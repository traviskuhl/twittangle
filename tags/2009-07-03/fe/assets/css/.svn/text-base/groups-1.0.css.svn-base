body.groups #page-content { 
	background: #fff;
}

#bd #groups-wrap {
	display: block !important;
	background: #fff;
}

body.groups .yui-gf form.group-create {
	margin-bottom: 15px;
}

body.groups .yui-gf form.group-create input {
	width: 110px;
	border: inset 1px #ccc;
}

body.groups .box a.gray {
	color: #888; 
	font-weight: bold;
}

body.groups .yui-gf .box h3 {
	font-weight: bold;
	font-size: 116%;
	color: #333;
}

body.groups .yui-gf .box h3,
body.groups .yui-gf .box .small {
	padding: 0 5px;
}

div.no-group {
	background: #fff;
	max-height: 400px;
	overflow: auto;
	margin-top: 5px;
	position: relative;
}

	div.no-group-item a, 	
	#bd div.no-group ul.user-list li a {
		display: block;
		background: transparent;
		text-decoration: none;
		margin: 5px;		
	}

	#bd div.no-group ul.user-list li {
		width: 170px;
		height: auto;
		border-bottom: solid 1px #eee;
		overflow: hidden;
	}

	div.no-group-item img, 	
	#bd div.no-group ul.user-list li img {
		width: 50px;
		height: 50px;
		float: left;
		background-repeat: no-repeat;
	}	

	div.no-group-item div,	
	div.no-group-item em,
	#bd div.no-group ul.user-list li em,
	#bd div.no-group ul.user-list li div {
		display: block;
		padding-left: 55px;
		font-weight: normal;
	}
	
	div.no-group-item em,
	#bd div.no-group ul.user-list li em {
		font-weight: bold;
	}	
	
	div.no-group-item {
		width: 170px;
		height: 55px;
	}
	
	
body.groups #bd ul.groups li {
	width: 45%;
	position: relative;
	float: left;
	height: 175px;
	border: solid 1px #ccc;
	margin: 0 10px 10px;
	padding: 0;
}

	body.groups #bd ul.groups li h3 {
		background: #ccc;
		padding: 5px;
		color: #333;
		font-size: 14px;	
		font-weight: bold;
		font-family: 'Lucida Grande',sans-serif;
	}

	body.groups #bd ul.groups li form {
		width: 130px;
		position: absolute;
		top: 4px;
		right: 10px;
	}

	body.groups #bd ul.groups li form input {
		border: inset 1px #eee;
		width: 130px;
		padding: 1px;
		color: #555;
	}
	
	body.groups #bd ul.groups li form .results ul li {
		width: auto;
		height: auto;
		border: none;
		padding: 2px;
		margin: 0 2px;
	}

	body.groups #bd ul.groups li div.in-group-wrap {
		width: 99%;
		max-height: 145px;
		overflow: auto;
		*height: 145px;
		position: relative;
		zoom: 1;
	}

	body.groups #bd ul.groups li div.in-group-wrap ul.mini-users {
		padding: 5px;
	}
	
	body.groups #bd ul.groups li div.in-group-wrap ul.mini-users li {
		width: 24px;
		height: 24px;
		margin: 3px;
	}
	
	body.groups #bd ul.groups li.none {
		width: 100%;
		border: none;
		text-align: center;
		padding-top: 50px;
	}
	
		body.groups #bd ul.groups li.none h3 {
			background: none;
			border: none;
		}
		
		body.groups #bd ul.groups li.none div {		
			color: #888;
			width: 400px;
			margin: 0 auto;
		}
	
	
#group-frame {
	position: relative;
}
	
#groups-saving {
	display: none;
	background: url(http://ms-cdn.com/static/moviestring/images/v1/arrow-loading.gif) no-repeat left center;
	color: #333;
	position: absolute;
	top: -40px;
	right: 10px;
	padding-left: 20px;
	font-weight: bold;
}	
 	
#groups-help-wrap {
	display: none;
	overflow: hidden;
	color: #444;
	padding: 20px;
	position: relative;
}

	#groups-help-wrap a {
		position: absolute;
		top: 0;
		right: 0;
		color: #888;
		font-weight: bold;
	}
	
	#groups-help-wrap h3 {
		padding: 10px 0 5px 10px;
		font-weight: bold;
	}	

	#groups-help-wrap ul {
		padding-left: 15px;
	}

.group-frame-loading #groups-saving {
	display: block;
}

.group-frame-loading ul.groups {
	opacity: .5;
	filter: alpha(opacity=50);
}	
	
#groups-wrap:after,	
#bd div.no-group ul.user-list li a:after,	
#bd div.no-group ul.user-list li:after { content:".";display:block;height:0;clear:both;visibility:hidden; font-size:0px; overflow: hidden} 		